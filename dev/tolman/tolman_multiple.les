n_subjects        = 1
mechanism         = tolman
behaviors         = b
stimulus_elements = s1, s2, s3, s4
alpha_z = 0.1
start_z = 0

@PHASE training stop: s1==50
S12 s1,s2 | S34
S34 s3,s4 | S12

@run training

xscale: all
@figure
@zplot s1->b->s3
@zplot s1->b->s4
@zplot s2->b->s3
@zplot s2->b->s4
@legend

@hexport hexport.txt