n_subjects        = 1
mechanism         = tolman
behaviors         = b
stimulus_elements = s1, s2, s3, s4
alpha_z = 1
start_z = 0
u = s4:1, default:0

@PHASE training stop: s1==100
S1 s1   | S2
S2 s2   | S3
S3 s3   | S4
S4 s4   | S1

@run training

xscale: s1
@figure
@zplot s1->b->s2
@zplot s2->b->s3
@zplot s3->b->s4
@legend

@hexport hexport.txt