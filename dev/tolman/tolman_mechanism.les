n_subjects        = 1
mechanism         = tolman
behaviors         = response
stimulus_elements = stimulus, reward
alpha_z = 0.2
start_z = 0.1

@PHASE training stop: stimulus==10
START_TRIAL stimulus   | REWARD
REWARD reward | START_TRIAL

@run training

xscale: all
@figure
@zplot stimulus->response->stimulus
@zplot stimulus->response->reward
@legend

@hexport hexport.txt